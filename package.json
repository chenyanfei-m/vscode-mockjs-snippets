{
    "name": "mockjs-snippets",
    "displayName": "Mockjs Snippets",
    "description": "mock.js snippets",
    "version": "0.0.6",
    "publisher": "chenyanfei-m",
    "engines": {
        "vscode": "^1.28.0"
    },
    "activationEvents": [
        "onCommand:extension.mockjsSearch"
    ],
    "main": "./out/extension.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/chenyanfei-m/vscode-mockjs-snippets.git"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "javascriptreact",
                "path": "./snippets/snippets.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/snippets.json"
            }
        ],
        "commands": [
            {
                "command": "extension.mockjsSearch",
                "title": "Mockjs: Search"
            }
        ]
    },
    "icon": "assets/image/icon.png"
}